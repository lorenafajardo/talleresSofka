

Proceso EjercicioII
	
	Definir i Como Entero;
	Definir e Como Entero;;
	Definir a Como Entero;;
	Definir n Como Entero;

	n <- 10;
	i<- 0;
	
	Mientras i <= n Hacer
		e<- 0;
		a<- 0;
		Mientras e < n-i Hacer
			Escribir Sin Saltar " ";
			e <- e+1;
		FinMientras
		Mientras a < i Hacer
			Escribir Sin Saltar "*";
			a <- a+1;
		FinMientras
		Escribir "";
		i<- i+1;
	FinMientras
	
FinProceso
