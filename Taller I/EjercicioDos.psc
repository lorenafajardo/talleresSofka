

// 2. Realizar un programa el cual solicite su nombre, apellidos, edad y estatura.

Proceso EjercicioDos
	
	Definir nombre Como Caracter;
	Definir apellidos Como Caracter;
	Definir edad Como Entero;
	Definir estatura Como Real;
	
	Escribir "Por favor diligencie la información solicitada ";
	Escribir " Nombre: ";
	Leer nombre;
	Escribir " Apellidos: ";
	Leer apellidos;
	Escribir " Edad: ";
	Leer edad;
	Escribir " Estatura: ";
	Leer estatura;
	
FinProceso
